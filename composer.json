{
    "name": "armincms/storage",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "armincms",
        "uploader",
        "media",
        "gallery",
        "storage"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem": "^1.0",
        "psr/http-message": "^1.0",
        "epartment/nova-dependency-container": "1.2.*", 
        "inspheric/nova-defaultable" : "^1.2",
        "inspheric/nova-url-field": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Storage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Storage\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
